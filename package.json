{
	"name": "tye2-vscode",
	"displayName": "Tye2",
	"description": "Makes it easy to debug and interact with applications using Tye2.",
	"version": "0.5.2",
	"preview": true,
	"icon": "resources/brand-tye_128x.png",
	"publisher": "tye-v2",
	"license": "SEE LICENSE IN LICENSE",
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"homepage": "https://github.com/tye-v2/tye2-vscode/",
	"repository": {
		"url": "https://github.com/tye-v2/tye2-vscode/"
	},
	"engines": {
		"tye2": ">=0.11.4",
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:workbench.action.tasks.runTask",
		"onDebugResolve:tye"
	],
	"main": "./main.js",
	"contributes": {
		"debuggers": [
			{
				"type": "tye2",
				"configurationAttributes": {
					"launch": {
						"properties": {
							"applicationName": {
								"type": "string",
								"description": "%tye2-vscode.debuggers.tye2.properties.applicationName.description%"
							},
							"services": {
								"type": "array",
								"description": "%tye2-vscode.debuggers.tye2.properties.services.description%",
								"items": {
									"type": "string"
								}
							},
							"watch": {
								"type": "boolean",
								"description": "%tye2-vscode.debuggers.tye2.properties.watch.description%"
							}
						},
						"required": [
							"applicationName"
						]
					}
				}
			}
		],
		"problemMatchers": [
			{
				"name": "tye2-run",
				"fileLocation": [
					"relative",
					"${workspaceFolder}"
				],
				"pattern": [
					{
						"regexp": ".",
						"file": 1,
						"location": 2,
						"message": 3
					}
				],
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^.*Executing application",
					"endsPattern": "^.*Listening for event pipe events"
				}
			}
		],
		"taskDefinitions": [
			{
				"type": "tye2-run",
				"properties": {
					"build": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.build.description%"
					},
					"dashboard": {
						"type": "boolean",
						"description": "%tye2-vscode.tasks.tye2-run.properties.dashboard.description%"
					},
					"debug": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.debug.description%"
					},
					"docker": {
						"type": "boolean",
						"description": "%tye2-vscode.tasks.tye2-run.properties.docker.description%"
					},
					"dtrace": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.dtrace.description%",
						"enum": [
							"zipkin"
						]
					},
					"framework": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.framework.description%"
					},
					"logs": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.logs.description%",
						"enum": [
							"console",
							"elastic",
							"ai",
							"seq"
						]
					},
					"metrics": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.metrics.description%"
					},
					"path": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.path.description%"
					},
					"port": {
						"type": "integer",
						"description": "%tye2-vscode.tasks.tye2-run.properties.port.description%"
					},
					"tags": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.tags.description%"
					},
					"verbosity": {
						"type": "string",
						"description": "%tye2-vscode.tasks.tye2-run.properties.verbosity.description%",
						"enum": [
							"Debug",
							"Info",
							"Quiet"
						]
					},
					"watch": {
						"type": "boolean",
						"description": "%tye2-vscode.tasks.tye2-run.properties.watch.description%"
					}
				}
			}
		],
		"viewsWelcome": [
			{
				"view": "tye2-vscode.views.services",
				"contents": "%tye2-vscode.views.services.contents.notInstalled%",
				"when": "tye2-vscode.views.services.state == 'notInstalled'"
			},
			{
				"view": "tye2-vscode.views.services",
				"contents": "%tye2-vscode.views.services.contents.notRunning%",
				"when": "tye2-vscode.views.services.state == 'notRunning'"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tye2-view",
					"title": "%tye2-vscode.activityBar.tye2-view.title%",
					"icon": "resources/brand-tye-white.svg"
				}
			]
		},
		"views": {
			"tye2-view": [
				{
					"id": "tye2-vscode.views.services",
					"name": "%tye2-vscode.views.services.name%"
				},
				{
					"id": "tye2-vscode.views.help",
					"name": "%tye2-vscode.views.help.name%"
				}
			]
		},
		"commands": [
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.refreshEntry",
				"title": "%tye2-vscode.commands.refreshEntry.title%",
				"icon": "$(refresh)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.debugAll",
				"title": "%tye2-vscode.commands.debugAll.title%",
				"icon": "$(debug-all)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.browseService",
				"title": "%tye2-vscode.commands.browseService.title%",
				"icon": "$(browser)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.attachService",
				"title": "%tye2-vscode.commands.attachService.title%",
				"icon": "$(debug-all)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.attachReplica",
				"title": "%tye2-vscode.commands.attachReplica.title%",
				"icon": "$(debug-alt)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.scaffolding.initTye2",
				"title": "%tye2-vscode.commands.scaffolding.initTye2.title%"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.launchTyeDashboard",
				"title": "%tye2-vscode.commands.launchTye2Dashboard.title%",
				"icon": "$(dashboard)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.scaffolding.scaffoldTye2Tasks",
				"title": "%tye2-vscode.commands.scaffolding.scaffoldTye2Tasks.title%"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.showLogs",
				"title": "%tye2-vscode.commands.showLogs.title%",
				"icon": "$(output)"
			},
			{
				"category": "Tye2",
				"command": "tye2-vscode.commands.shutdownApplication",
				"title": "%tye2-vscode.commands.shutdownApplication.title%",
				"icon": "$(debug-stop)"
			},
			{
				"command": "tye2-vscode.commands.help.getStarted",
				"title": "%tye2-vscode.commands.help.getStarted.title%",
				"category": "Tye2"
			},
			{
				"command": "tye2-vscode.commands.help.installTye2",
				"title": "%tye2-vscode.commands.help.installTye2.title%",
				"category": "Tye2"
			},
			{
				"command": "tye2-vscode.commands.help.readDocumentation",
				"title": "%tye2-vscode.commands.help.readDocumentation.title%",
				"category": "Tye2"
			},
			{
				"command": "tye2-vscode.commands.help.reportIssue",
				"title": "%tye2-vscode.commands.help.reportIssue.title%",
				"category": "Tye2"
			},
			{
				"command": "tye2-vscode.commands.help.reviewIssues",
				"title": "%tye2-vscode.commands.help.reviewIssues.title%",
				"category": "Tye2"
			}
		],
		"configuration": {
			"title": "Tye2",
			"properties": {
				"tye2.paths.tye2Path": {
					"type": "string",
					"description": "%tye2-vscode.configuration.paths.tye2Path.description%"
				}
			}
		},
		"menus": {
			"commandPalette": [
				{
					"command": "tye2-vscode.commands.attachService",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.attachReplica",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.browseService",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.debugAll",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.refreshEntry",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.showLogs",
					"when": "never"
				},
				{
					"command": "tye2-vscode.commands.shutdownApplication",
					"when": "never"
				}
			],
			"view/title": [
				{
					"command": "tye2-vscode.commands.refreshEntry",
					"when": "view == tye2-vscode.views.services",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "tye2-vscode.commands.launchTyeDashboard",
					"when": "view == tye2-vscode.views.services && viewItem =~ /application/",
					"group": "inline@1"
				},
				{
					"command": "tye2-vscode.commands.debugAll",
					"when": "view == tye2-vscode.views.services && viewItem =~ /application/",
					"group": "inline@2"
				},
				{
					"command": "tye2-vscode.commands.shutdownApplication",
					"when": "view == tye2-vscode.views.services && viewItem =~ /application/",
					"group": "inline@3"
				},
				{
					"command": "tye2-vscode.commands.browseService",
					"when": "view == tye2-vscode.views.services && viewItem =~ /browsable/",
					"group": "inline"
				},
				{
					"command": "tye2-vscode.commands.attachService",
					"when": "view == tye2-vscode.views.services && viewItem =~ /attachable/ && viewItem =~ /service/",
					"group": "inline"
				},
				{
					"command": "tye2-vscode.commands.attachReplica",
					"when": "view == tye2-vscode.views.services && viewItem =~ /attachable/ && viewItem =~ /replica/",
					"group": "inline"
				},
				{
					"command": "tye2-vscode.commands.showLogs",
					"when": "view == tye2-vscode.views.services && viewItem =~ /hasLogs/",
					"group": "inline"
				}
			]
		},
		"yamlValidation": [
			{
				"fileMatch": [
					"tye.yml",
					"tye.yaml"
				],
				"url": "./resources/schemas/tye.json"
			}
		]
	},
	"scripts": {
		"build": "webpack --mode production --devtool hidden-source-map",
		"build-test": "tsc -p ./",
		"lint": "eslint --max-warnings 0 src --ext ts",
		"watch": "webpack --watch",
		"package": "npm run build && npm run package-impl",
		"ci-build": "npm run lint && npm run build",
		"ci-package": "npm run ci-build && npm run package-impl",
		"test": "npm run build-test && node ./out/test/runTest.js",
		"package-impl": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/js-yaml": "^4.0.5",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.11.38",
		"@types/node-netstat": "^1.8.1",
		"@types/ps-tree": "^1.1.2",
		"@types/semver": "^7.3.9",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.2.0",
		"@vscode/vsce": "^2.18.0",
		"eslint": "^8.36.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"terser-webpack-plugin": "^5.3.7",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.3",
		"vscode-nls-dev": "^4.0.3",
		"webpack": "^5.76.2",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@microsoft/vscode-azext-utils": "^0.5.1",
		"axios": "^1.3.4",
		"js-yaml": "^4.1.0",
		"node-netstat": "^1.8.0",
		"ps-list": "^7.2.0",
		"ps-tree": "^1.2.0",
		"rxjs": "^7.8.0",
		"semver": "^7.3.8",
		"vscode-nls": "^5.2.0"
	},
	"overrides": {
		"xml2js": "0.5.0"
	}
}
